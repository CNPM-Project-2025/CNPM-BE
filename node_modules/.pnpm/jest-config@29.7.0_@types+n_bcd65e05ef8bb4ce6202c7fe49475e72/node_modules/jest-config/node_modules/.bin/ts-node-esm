#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Hoc/OneDrive/Tài liệu/Dự Án/CNPM/CNPM-BE/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._a50f76f4682345dbaa6e5f9a5e61edfd/node_modules/ts-node/dist/node_modules:/mnt/c/Users/Hoc/OneDrive/Tài liệu/Dự Án/CNPM/CNPM-BE/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._a50f76f4682345dbaa6e5f9a5e61edfd/node_modules/ts-node/node_modules:/mnt/c/Users/Hoc/OneDrive/Tài liệu/Dự Án/CNPM/CNPM-BE/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._a50f76f4682345dbaa6e5f9a5e61edfd/node_modules:/mnt/c/Users/Hoc/OneDrive/Tài liệu/Dự Án/CNPM/CNPM-BE/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Hoc/OneDrive/Tài liệu/Dự Án/CNPM/CNPM-BE/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._a50f76f4682345dbaa6e5f9a5e61edfd/node_modules/ts-node/dist/node_modules:/mnt/c/Users/Hoc/OneDrive/Tài liệu/Dự Án/CNPM/CNPM-BE/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._a50f76f4682345dbaa6e5f9a5e61edfd/node_modules/ts-node/node_modules:/mnt/c/Users/Hoc/OneDrive/Tài liệu/Dự Án/CNPM/CNPM-BE/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._a50f76f4682345dbaa6e5f9a5e61edfd/node_modules:/mnt/c/Users/Hoc/OneDrive/Tài liệu/Dự Án/CNPM/CNPM-BE/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ts-node@10.9.2_@swc+core@1._a50f76f4682345dbaa6e5f9a5e61edfd/node_modules/ts-node/dist/bin-esm.js" "$@"
else
  exec node  "$basedir/../../../../../ts-node@10.9.2_@swc+core@1._a50f76f4682345dbaa6e5f9a5e61edfd/node_modules/ts-node/dist/bin-esm.js" "$@"
fi
